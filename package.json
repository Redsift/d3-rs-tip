{
  "name": "@redsift/d3-rs-tip",
  "version": "0.2.0",
  "description": "Tip for graphs using D3v4.",
  "keywords": [
    "redsift",
    "d3",
    "tip"
  ],
  "homepage": "https://github.com/redsift/d3-rs-tip",
  "license": "MIT",
  "author": {
    "name": "Christos Vontas"
  },
  "main": "distribution/d3-rs-tip.umd-es2015.min.js",
  "jsnext:main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-tip.git"
  },
  "scripts": {
    "serve": "gulp -o d3-rs-tip -g d3-selection -g d3-collection -g @redsift/d3-rs-svg serve",
    "build": "gulp -o d3-rs-tip -g d3-selection -g d3-collection -g @redsift/d3-rs-svg build",
    "pretest": "npm outdated && npm run build",
    "test": "tape 'test/**/*-test.js' && eslint index.js src",
    "version": "json2module package.json > package.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@redsift/d3-rs-svg": "~0.2.3",
    "d3-selection": "~0.8.0"
  },
  "devDependencies": {
    "@redsift/tape-reel": "~0.0.2",
    "babel-preset-es2015-rollup": "^1.1.0",
    "browser-sync": "^2.13.0",
    "del": "^2.2.0",
    "eslint": "^2.10.2",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "json2module": "~0.0.3",
    "rollup": "^0.26",
    "rollup-plugin-babel": "^2.5.1",
    "rollup-plugin-includepaths": "^0.1.2",
    "rollup-stream": "^1.6.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^4.7.1"
  }
}
